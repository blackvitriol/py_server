    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server",
            "type": "python",
            "cwd": "${workspaceFolder}\\server",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "server:app"
            ],
            "jinja": true,
            "justMyCode": true
        },
        {
            "name": "Run User Interface",
            "type": "python",
            "request": "launch",
            "program": "interface\\interface.py",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}